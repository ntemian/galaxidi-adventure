  },
  {
    "file": "narrator/098.mp3",
    "voice_id": "pqHfZKP75CvOlQylNhV4",
    "speaker": "",
    "text": "Το χαρτί μυρίζει θάλασσα. Παλιομοδίτικο γράψιμο.",
